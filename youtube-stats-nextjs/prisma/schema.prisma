model Channel {
  id          Int      @id @default(autoincrement())
  name        String
  url         String   @unique
  subscriberCount Int
  totalViews  Int
  totalVideos Int
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt
}

model Video {
  id          Int      @id @default(autoincrement())
  channelId   Int
  title       String
  views       Int
  publishedAt DateTime
  createdAt   DateTime @default(now())
  updatedAt   DateTime @updatedAt

  channel     Channel   @relation(fields: [channelId], references: [id])
}